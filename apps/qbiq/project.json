{
  "name": "qbiq",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/qbiq/src",
  "projectType": "application",
  "tags": ["prismql", "prismxl", "prisma", "prism"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/qbiq",
        "main": "apps/qbiq/src/main.ts",
        "tsConfig": "apps/qbiq/tsconfig.app.json",
        "assets": ["apps/qbiq/src/assets"],
        "webpackConfig": "apps/qbiq/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "qbiq:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "qbiq:build:development"
        },
        "production": {
          "buildTarget": "qbiq:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/qbiq/jest.config.ts"
      }
    }
  }
}
